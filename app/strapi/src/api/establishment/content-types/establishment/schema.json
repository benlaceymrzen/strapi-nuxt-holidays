{
  "kind": "collectionType",
  "collectionName": "establishments",
  "info": {
    "singularName": "establishment",
    "pluralName": "establishments",
    "displayName": "Establishments",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "text"
    },
    "postal_code": {
      "type": "string"
    },
    "phone_number": {
      "type": "string"
    },
    "rating": {
      "type": "integer",
      "default": 0,
      "max": 5,
      "min": 1
    },
    "fax_number": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "location": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::location.location",
      "inversedBy": "establishments"
    },
    "accommodation_type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::accommodation-type.accommodation-type"
    },
    "rating_type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::rating-type.rating-type"
    },
    "establishment_id": {
      "type": "uid",
      "default": "0",
      "required": true
    },
    "facilities": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::facility.facility",
      "inversedBy": "establishments"
    },
    "geographic_location": {
      "type": "component",
      "repeatable": false,
      "component": "geographic.geo-point"
    },
    "description": {
      "type": "text",
      "required": false
    }
  }
}
