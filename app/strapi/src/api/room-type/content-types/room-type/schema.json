{
  "kind": "collectionType",
  "collectionName": "room_types",
  "info": {
    "singularName": "room-type",
    "pluralName": "room-types",
    "displayName": "Room Types",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "room_code": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "establishments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::establishment.establishment",
      "mappedBy": "room_type_id"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "image_id": {
      "type": "string",
      "required": true,
      "unique": false
    }
  }
}
